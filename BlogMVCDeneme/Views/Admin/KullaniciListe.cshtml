@model IEnumerable<BlogMVCDeneme.ViewModels.Kullanici>

@{
    ViewBag.Title = "KullaniciListe";
}

<h2>KullaniciListe</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Soyad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Soyad)
        </td>
        <td>
            @Html.ActionLink("Rol Tanımla", "../Manage/KullaniciRolEkle", new { id=item.Id }) |
           
        </td>
    </tr>
}

</table>
